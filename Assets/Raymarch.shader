				float4 transform17 = mul(GetObjectToWorldMatrix(),float4( 0,0,0,1 ));
				float3 offset6 = ( float4( _offset , 0.0 ) - transform17 ).xyz;
				int numLightSteps6 = _NumLightSteps;
